            <Item Name="pileup_directory_data_down" Value="../common/data/2018/MyDataPileupHistogram2018_66017.root" />
	    <Item Name="sample_name" Value="ZprimeToZhToZhadhbb_narrow_M-2000_13TeV-madgraph" />
	    <Item Name="jet_sorting" Value="random" /> <!--options: btag,random !-->	    	    
            <Item Name="TestKey" Value="TestKeyValue" />
            <Item Name="jersmear_direction" Value="up"/>
        </UserConfig>
    </Cycle>
</JobConfiguration>
